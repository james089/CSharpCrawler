<Window x:Class="CSharpCrawler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharpCrawler"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="24" Margin="5" TextWrapping="Wrap" Text="Search Content" VerticalAlignment="Top" Width="120"/>
            <Button x:Name="Btn_download" Content="Button" HorizontalAlignment="Left" Margin="5" VerticalAlignment="Top" Width="auto"/>
        </DockPanel>

    </Grid>
</Window>
